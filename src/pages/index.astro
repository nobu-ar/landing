---
import Layout from "../layouts/Layout.astro";
import { Hero } from "../components/Hero.jsx";
import { Navbar } from "../components/Navbar.jsx";
import { Features1 } from "../components/Features1.jsx";
import { Modulos } from "../components/Modulos.jsx";
import { Testimonials } from "../components/Testimonials.jsx";
import { FeaturesDiagonal } from "../components/FeaturesDiagonal.jsx";
import { Pricing } from "../components/Pricing.jsx";
import { FAQ } from "../components/FAQ.jsx";
import { Brands } from "../components/Brands.jsx";
import { Divider } from "../components/Divider";
import { Footer } from "../components/Footer.jsx";
import { ScrollUpButton } from "../components/ScrollUpButton";
import "../styles/Theme.css";
import "../styles/Diagonals.css";
import { PersonalSection} from "../components/sections/PersonalSection.jsx"
import { MutualSection} from "../components/sections/MutualSection.jsx"
import { FinanceSection} from "../components/sections/FinanceSection.jsx"
import { CommerceSection} from "../components/sections/CommerceSection.jsx"
import { FaltasSection} from "../components/sections/FaltasSection.jsx"






---

<Layout title="Nobu">
  <Navbar client:load />
  <Hero client:load class="mt-20" />
  <Features1 client:load />
  <Modulos client:load />
  <div id="personalSection">
    <PersonalSection client:load />
  </div>
  <div id="mutualSection">
    <MutualSection client:load />
  </div>
  <div id="commerceSection">
    <CommerceSection client:load />
  </div>
  <div id="financeSection">
    <FinanceSection client:load />
  </div>
  <div id="faltasSection">
    <FaltasSection client:load />
  </div>
  <FeaturesDiagonal client:load />
  <Footer />
  <ScrollUpButton client:load />
</Layout>
